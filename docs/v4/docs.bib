@misc{ng_prediction_2015,
	type = {Research {Article}},
	title = {Prediction of {Antimicrobial} {Peptides} {Based} on {Sequence} {Alignment} and {Support} {Vector} {Machine}-{Pairwise} {Algorithm} {Utilizing} {LZ}-{Complexity}},
	url = {https://www.hindawi.com/journals/bmri/2015/212715/},
	abstract = {This study concerns an attempt to establish a new method for predicting antimicrobial peptides (AMPs) which are important to the immune system. Recently, researchers are interested in designing alternative drugs based on AMPs because they have found that a large number of bacterial strains have become resistant to available antibiotics. However, researchers have encountered obstacles in the AMPs designing process as experiments to extract AMPs from protein sequences are costly and require a long set-up time. Therefore, a computational tool for AMPs prediction is needed to resolve this problem. In this study, an integrated algorithm is newly introduced to predict AMPs by integrating sequence alignment and support vector machine- (SVM-) LZ complexity pairwise algorithm. It was observed that, when all sequences in the training set are used, the sensitivity of the proposed algorithm is 95.28\% in jackknife test and 87.59\% in independent test, while the sensitivity obtained for jackknife test and independent test is 88.74\% and 78.70\%, respectively, when only the sequences that has less than 70\% similarity are used. Applying the proposed algorithm may allow researchers to effectively predict AMPs from unknown protein peptide sequences with higher sensitivity.},
	language = {en},
	urldate = {2020-02-14},
	journal = {BioMed Research International},
	author = {Ng, Xin Yi and Rosdi, Bakhtiar Affendi and Shahrudin, Shahriza},
	year = {2015},
	doi = {https://doi.org/10.1155/2015/212715}
}

@article{porto_cs-amppred_2012,
	title = {{CS}-{AMPPred}: {An} {Updated} {SVM} {Model} for {Antimicrobial} {Activity} {Prediction} in {Cysteine}-{Stabilized} {Peptides}},
	volume = {7},
	issn = {1932-6203},
	shorttitle = {{CS}-{AMPPred}},
	url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0051444},
	doi = {10.1371/journal.pone.0051444},
	abstract = {The antimicrobial peptides (AMP) have been proposed as an alternative to control resistant pathogens. However, due to multifunctional properties of several AMP classes, until now there has been no way to perform efficient AMP identification, except through in vitro and in vivo tests. Nevertheless, an indication of activity can be provided by prediction methods. In order to contribute to the AMP prediction field, the CS-AMPPred (Cysteine-Stabilized Antimicrobial Peptides Predictor) is presented here, consisting of an updated version of the Support Vector Machine (SVM) model for antimicrobial activity prediction in cysteine-stabilized peptides. The CS-AMPPred is based on five sequence descriptors: indexes of (i) α-helix and (ii) loop formation; and averages of (iii) net charge, (iv) hydrophobicity and (v) flexibility. CS-AMPPred was based on 310 cysteine-stabilized AMPs and 310 sequences extracted from PDB. The polynomial kernel achieves the best accuracy on 5-fold cross validation (85.81\%), while the radial and linear kernels achieve 84.19\%. Testing in a blind data set, the polynomial and radial kernels achieve an accuracy of 90.00\%, while the linear model achieves 89.33\%. The three models reach higher accuracies than previously described methods. A standalone version of CS-AMPPred is available for download at {\textless}http://sourceforge.net/projects/csamppred/{\textgreater} and runs on any Linux machine.},
	language = {en},
	number = {12},
	urldate = {2020-02-14},
	journal = {PLOS ONE},
	author = {Porto, William F. and Pires, Állan S. and Franco, Octavio L.},
	month = dec,
	year = {2012},
	keywords = {Antimicrobials, Cysteine, Database searching, Forecasting, Polynomials, Sequence databases, Sequence similarity searching, Support vector machines},
	pages = {e51444},
	file = {Snapshot:/home/chrisp/Documents/papers/zotero/storage/FV4R96CJ/Porto et al. - 2012 - CS-AMPPred An Updated SVM Model for Antimicrobial.html:text/html;Full Text PDF:/home/chrisp/Documents/papers/zotero/storage/4C2SCSPK/Porto et al. - 2012 - CS-AMPPred An Updated SVM Model for Antimicrobial.pdf:application/pdf}
}

