	pages = {161--169},
	file = {ScienceDirect Full Text PDF:/home/chrisp/Documents/papers/zotero/storage/TQE96PCJ/Tompa et al. - 2014 - A Million Peptide Motifs for the Molecular Biologi.pdf:application/pdf;ScienceDirect Snapshot:/home/chrisp/Documents/papers/zotero/storage/FTNF57HI/Tompa et al. - 2014 - A Million Peptide Motifs for the Molecular Biologi.html:text/html}
}

